@page
@using Hillerød_Sejklub.Models
@model Hillerød_Sejklub.Models.BoatsModel
@{
    ViewData["Title"] = "Både";
}
<h1>List of Boats</h1>
<p>Here you can find a list of all the boats available in Hillerød Sejklub.</p>
<table border="1" cellpadding=" 5" cellspacing=" 0">
    <thead>
        <tr>   

            <th>Boat Name</th>
            <th>Model</th>
            <th>Sail Number</th>
            <th>Engine info</th>
            <th>Build year</th>
            <th>Available</th>

        </tr>
    </thead>

    <tbody>
        @foreach (Hillerød_Sejklub.Pages.Boats boat in Model.BoatsList)
        {
		<tr>
					<td>@boat.Name</td>
                    <td>@boat.Model</td>
                    <td>@boat.SailNumber</td>
                    <td>@boat.EngineInfo</td>
                    <td>@boat.BuildYear</td>
                    <td>@(boat.IsAvailable ? "Yes" : "No")</td>


		</tr>
			}
        </tbody>
	</table>

